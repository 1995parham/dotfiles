{
  "experimental": true,
  // reserved 10.254.1.1/24 (254 IP addresses) for the bridge network
  "bip": "10.254.1.1/24",
  // for any other network, docker will partition up the 10.254.0.0 space (65k hosts), giving out 16 hosts at a time
  // ("size":28 refers to the CIDR mask, for 16 hosts).
  "default-address-pools": [
    {
      "base": "10.254.0.0/16",
      "size": 24
    },
    {
      "base": "10.254.2.0/16",
      "size": 24
    },
    {
      "base": "10.254.3.0/16",
      "size": 24
    }
  ],
  "features": {
    "buildkit": true
  }
}

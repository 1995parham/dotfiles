{
  "position": "bottom",
  "ipc": true,
  "id": "bar-0",
  "height": 33,
  "modules-center": [
    "custom/love#relationship",
    "idle_inhibitor",
    "custom/love#birthday"
  ],
  "modules-left": [
    "custom/date",
    "custom/jdate",
    "custom/khal",
    "network#alvani",
    "network#home"
  ],
  "modules-right": [
    "network#snapp",
    "network#baly",
    "network#aut",
    "network#offerland",
    "custom/touchpad"
  ],
  "idle_inhibitor": {
    "format": "{icon}",
    "format-icons": {
      "activated": "",
      "deactivated": ""
    }
  },
  "bluetooth": {
    "format": "{icon}",
    "format-alt": "bluetooth: {status}",
    "format-icons": {
      "enabled": "",
      "disabled": ""
    },
    "tooltip-format": "{}"
  },
  "network#alvani": {
    "interface": "alvani",
    "format": "{ipaddr} 󰋒",
    "format-disconnected": ""
  },
  "network#home": {
    "interface": "home",
    "format": "{ipaddr} ",
    "format-disconnected": ""
  },
  "network#snapp": {
    "interface": "snapp-*",
    "format": "{ifname}: {ipaddr}",
    "format-disconnected": ""
  },
  "network#baly": {
    "interface": "baly",
    "format": "{ifname}: {ipaddr}",
    "format-disconnected": ""
  },
  "network#aut": {
    "interface": "aut",
    "format": "{ifname}: {ipaddr}",
    "format-disconnected": ""
  },
  "network#offerland": {
    "interface": "offerland",
    "format": "{ifname}: {ipaddr}",
    "format-disconnected": ""
  },
  "custom/touchpad": {
    "interval": 3,
    "exec": "$HOME/.config/waybar/scripts/touchpad.sh"
  },
  "custom/love#relationship": {
    "interval": 3600,
    "exec": "$HOME/.config/waybar/scripts/couple.sh relationship",
    "format": " {}"
  },
  "custom/love#birthday": {
    "interval": 3600,
    "exec": "$HOME/.config/waybar/scripts/couple.sh birthday",
    "format": " {}"
  },
  "custom/date": {
    "interval": 3600,
    "exec": "date +'%d %B %Y'",
    "exec-if": "which date",
    "format": "{} "
  },
  "custom/jdate": {
    "interval": 3600,
    "exec": "jdate +'%d %B %Y'",
    "exec-if": "which jdate",
    "format": "{} "
  },
  "custom/khal": {
    "format": "{}",
    "tooltip": true,
    "interval": 300,
    "format-icons": {
      "default": " "
    },
    "exec": "$HOME/.config/waybar/scripts/khal.py",
    "return-type": "json"
  }
}

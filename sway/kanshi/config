profile aut-teaching-201 {
  # AUT 201 class projector (setup on 20 Feb 2023)
  output "Panasonic Industry Company PT-LB385 Unknown" enable
  output eDP-1 enable

  exec swaymsg 'workspace --no-auto-back-and-forth $wsf', move workspace to output '"Panasonic Industry Company PT-LB385 Unknown"'
}

